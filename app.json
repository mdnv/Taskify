{
  "expo": {
    "name": "Taskify",
    "slug": "taskify",
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./assets/taskify.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/taskify-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#6366F1"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.iliyar.taskify",
      "versionCode": 4,
      "adaptiveIcon": {
        "foregroundImage": "./assets/taskify-adaptive.png",
        "backgroundColor": "#6366F1"
      },
      "permissions": []
    },
    "web": {
      "favicon": "./assets/taskify.png"
    },
    "extra": {
      "eas": {
        "projectId": "408b89b2-13da-48a8-91f7-f2e24d01a03a"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 33,
            "targetSdkVersion": 33,
            "buildToolsVersion": "33.0.0"
          }
        }
      ],
      [
        "react-native-android-widget",
        {
          "widgets": [
            {
              "name": "TaskifyWidget",
              "label": "Taskify",
              "minWidth": 240,
              "minHeight": 240,
              "maxWidth": 500,
              "maxHeight": 600,
              "previewImage": "./assets/taskify.png",
              "description": "View and manage your tasks"
            }
          ]
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/408b89b2-13da-48a8-91f7-f2e24d01a03a"
    }
  }
}
